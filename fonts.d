int get_font_offset(ushort val)
{
	int result=0;
	if(val>=0x2580 && val<=0x259F){
		int offset=val-0x2580;
		result=8*12*(256+offset);
	}else{
		int i;
		for(i=0;i<utf8_lookup.length/2;i++){
			int index=i*2;
			if(utf8_lookup[index+1]==val){
				result=utf8_lookup[index];
				result*=8*12;
				break;
			}
		}
		result=(val&0xFF)*8*12;
	}
	return result;
}

import core.sys.windows.windows;
HFONT get_dejavu_font()
{
	static HFONT hf=NULL;
	if(hf is null){
		string font_data=import("DejaVuSansMono.ttf");
		static HANDLE htmp;
		if(htmp is null){
			DWORD num;
			htmp=AddFontMemResourceEx(cast(void*)font_data.ptr,font_data.length,null,&num);
		}
		hf=cast(HFONT)CreateFont(-15,0,0,0,FW_NORMAL,0,0,0,0,0,0,0,0,"DejaVu Sans Mono");
	}
	return hf;
}